{
  "name": "jurify-legal-saas",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "ðŸš€ Jurify - Legal SaaS Platform with AI Agents (Elon Musk/xAI Standard)",
  "author": "Jurify Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:https": "vite --mode development --https",
    "dev:debug": "vite --mode development --debug",
    "dev:force": "vite --mode development --force",
    "dev:8080": "vite --mode development --port 8080",
    "dev:force:8080": "vite --mode development --force --port 8080",
    "build": "npm run type-check && vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit --skipLibCheck false",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:security": "npm audit && npm run test:security:custom",
    "test:security:custom": "node scripts/security-audit.js",
    "test:performance": "lighthouse-ci autorun",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.types.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase seed run",
    "db:backup": "node scripts/backup-database.js",
    "preview": "vite preview --port 4173 --https",
    "deploy:staging": "npm run build:staging && node scripts/deploy-staging.js",
    "deploy:production": "npm run pre-deploy && npm run build && node scripts/deploy-production.js",
    "pre-deploy": "npm run lint && npm run type-check && npm run test && npm run test:security",
    "health-check": "node scripts/health-check.js",
    "monitor": "node scripts/monitor-performance.js",
    "logs": "node scripts/fetch-logs.js",
    "cleanup": "node scripts/cleanup-cache.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze:bundle": "npx webpack-bundle-analyzer dist/stats.json",
    "analyze:deps": "npx depcheck",
    "update:deps": "npx npm-check-updates -u"
  },
